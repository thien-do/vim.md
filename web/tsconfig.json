{
	"extends": "../tsconfig.base.json",
	"include": ["src"],
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext"],
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"noFallthroughCasesInSwitch": false,
		"allowJs": false,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "./src"
	}
}
